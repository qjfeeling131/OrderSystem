@{
    Layout = "~/views/template/content.cshtml";
}


@section title{ 营销远程平台 | 错误 }


@section content_header{

<section class="content-header">
    <h1>
        错误页面
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="glyphicon glyphicon-home"></i> 主页</a></li>
        <li class="active">错误</li>
    </ol>
</section>
}


@section content{
<section class="content">

    <div class="error-page">
        <h2 class="headline text-red">500</h2>
        <div class="error-content">
            <h3><i class="glyphicon glyphicon-warning-sign text-red"></i> 异常！</h3>
            <p>
                服务器繁忙，<a href="javascript:history.back()">返回</a> <br />
                登陆超时，身份验证失败，<br />您可尝试
                <a href="@Url.Content("~/home/login")">请重新登陆</a> ！<br />
            </p>

        </div>
    </div><!-- /.error-page -->

</section>

}